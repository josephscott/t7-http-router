{
	"name": "t7/http-router",
	"license": "MIT",
	"require": {
		"php": ">=8.3",
		"nikic/fast-route": "^1.3.0"
	},
	"require-dev": {
		"friendsofphp/php-cs-fixer": "^3.64",
		"josephscott/phpcsfixer-config": "^0.0.3",
		"phpstan/phpstan": "^1.12",
		"pestphp/pest": "^3.5.0",
		"t7/http_request": "^0.0.1"
	},
	"autoload": {
		"classmap": [
			"src/t7/http/router.php",
			"src/t7/http/route-handler.php"
		]
	},
	"config": {
		"classmap-authoritative": true,
		"optimize-autoloader": true,
		"allow-plugins": false
	}
}
